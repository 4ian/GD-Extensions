<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="SoundObjectEditor">
		<title>Edit the Sound object</title>
		<centered>1</centered>
		<size>399,399</size>
		<id_arg>0</id_arg>
		<style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER</style>
		<object class="wxFlexGridSizer" variable="FlexGridSizer3" member="no">
			<cols>1</cols>
			<rows>3</rows>
			<growablecols>0</growablecols>
			<growablerows>0</growablerows>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
					<cols>1</cols>
					<rows>2</rows>
					<growablecols>0</growablecols>
					<growablerows>1</growablerows>
					<object class="sizeritem">
						<object class="wxNotebook" name="ID_NOTEBOOK1" variable="Notebook1" member="yes">
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
									<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
										<cols>1</cols>
										<growablecols>0</growablecols>
										<growablerows>1</growablerows>
										<object class="sizeritem">
											<object class="wxStaticBoxSizer" variable="StaticBoxSizer4" member="no">
												<label>Sound type</label>
												<object class="sizeritem">
													<object class="wxRadioButton" name="ID_RADIOBUTTON2" variable="MusicRadioBt" member="yes">
														<label>Music</label>
														<tooltip>Load the content of the sound file only partially when the sound is played. Useful for long sounds and musics.</tooltip>
														<style>wxRB_GROUP</style>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxRadioButton" name="ID_RADIOBUTTON1" variable="SoundRadioBt" member="yes">
														<label>Sound</label>
														<tooltip>The sound file is entirely loaded into memory so as to be quicly played. Adapted to small sounds and noises.</tooltip>
													</object>
													<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="spacer">
											<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxStaticBoxSizer" variable="StaticBoxSizer3" member="no">
												<label>Note</label>
												<object class="sizeritem">
													<object class="wxFlexGridSizer" variable="FlexGridSizer8" member="no">
														<cols>1</cols>
														<object class="sizeritem">
															<object class="wxStaticText" name="ID_STATICTEXT6" variable="StaticText6" member="yes">
																<label>Le mode &quot;Musique&quot; est plus approprié pour les fichier son qui durent plus de 10 sec. Le&#x0A;fichier son n&apos;est chargé que partiellement au fur et à mesure de la lecture.&#x0A;Le mode &quot;Son&quot; est adapté au petits fichiers son. Le son est chargé totalement et peu&#x0A;être joué sans aucun délai. Toutefois, ce  mode ralentit fortement le jeu pour des&#x0A;grands fichiers.</label>
																<font>
																	<size>8</size>
																</font>
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxStaticText" name="ID_STATICTEXT7" variable="StaticText7" member="yes">
																<label>La spatialisation du son n&apos;est possible que pour les sons &quot;monos&quot;.</label>
																<font>
																	<size>8</size>
																</font>
															</object>
															<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxALL|wxEXPAND|wxALIGN_BOTTOM|wxALIGN_CENTER_HORIZONTAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
								</object>
								<label>Type</label>
							</object>
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="yes">
									<object class="wxFlexGridSizer" variable="FlexGridSizer6" member="no">
										<cols>1</cols>
										<growablecols>0</growablecols>
										<object class="sizeritem">
											<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
												<label>Properties</label>
												<object class="sizeritem">
													<object class="wxFlexGridSizer" variable="FlexGridSizer4" member="no">
														<cols>2</cols>
														<rows>5</rows>
														<growablecols>1</growablecols>
														<object class="sizeritem">
															<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
																<label>Sound level:</label>
															</object>
															<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxSpinCtrl" name="ID_SPINCTRL1" variable="VolumeSpinCtrl" member="yes">
																<value>0</value>
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
																<label>Pitch:</label>
															</object>
															<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="pitchTextCtrl" member="yes">
																<value>1</value>
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
																<label>Minimal distance:</label>
															</object>
															<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxSpinCtrl" name="ID_SPINCTRL3" variable="MinDistanceSpinCtrl" member="yes">
																<value>1</value>
																<min>1</min>
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="yes">
																<label>Attenuation:</label>
															</object>
															<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="AttenuationSpinCtrl" member="yes">
																<value>1</value>
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="spacer">
															<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxCheckBox" name="ID_CHECKBOX1" variable="LoopCheckBox" member="yes">
																<label>Loop</label>
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
													</object>
													<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxStaticBoxSizer" variable="StaticBoxSizer2" member="no">
												<label>Filename</label>
												<object class="sizeritem">
													<object class="wxFlexGridSizer" variable="FlexGridSizer5" member="no">
														<cols>3</cols>
														<rows>1</rows>
														<growablecols>1</growablecols>
														<object class="sizeritem">
															<object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText5" member="yes">
																<label>Name of the sound file:</label>
															</object>
															<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="FileNameTextCtrl" member="yes">
																<size>99,21</size>
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxButton" name="ID_BUTTON2" variable="Button1" member="yes">
																<label>...</label>
																<size>30,23</size>
																<tooltip>Browse</tooltip>
																<handler function="OnButton1Click" entry="EVT_BUTTON" />
															</object>
															<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
															<border>5</border>
															<option>1</option>
														</object>
													</object>
													<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
								</object>
								<label>Sound/Music</label>
							</object>
						</object>
						<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticLine" name="ID_STATICLINE1" variable="StaticLine1" member="yes">
					<size>10,-1</size>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer7" member="no">
					<cols>3</cols>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="ValidateButton" member="yes">
							<label>Ok</label>
							<handler function="OnValidateButtonClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON3" variable="Button2" member="yes">
							<label>Cancel</label>
							<handler function="OnButton2Click" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>







